<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="lazyLoadingEnabled" value="false"/>
    </settings>

    <typeAliases>
        <typeAlias alias="User" type="com.mfq.bean.User"/>
        <typeAlias alias="UserQuota" type="com.mfq.bean.UserQuota"/>
        <typeAlias alias="Product" type="com.mfq.bean.Product"/>
        <typeAlias alias="ProductDetail" type="com.mfq.bean.ProductDetail"/>
        <typeAlias alias="ProductClassify" type="com.mfq.bean.ProductClassify"/>
        <typeAlias alias="SysPassport" type="com.mfq.bean.SysPassport"/>
        <typeAlias alias="OpLog" type="com.mfq.bean.OpLogs"/>
        <typeAlias alias="Hospital" type="com.mfq.bean.Hospital"/>
        <typeAlias alias="OrderInfo" type="com.mfq.bean.OrderInfo"/>
        <typeAlias alias="WeChatMsg" type="com.mfq.bean.WechatMsg"/>
        <typeAlias alias="UserExtend" type="com.mfq.bean.UserExtend"/>
        <typeAlias alias="Notification" type="com.mfq.bean.Notification"/>
        <typeAlias alias="ProductImg" type="com.mfq.bean.ProductImg"/>
        <typeAlias alias="HomeBanner" type="com.mfq.bean.HomeBanner"/>
        <typeAlias alias="HomeClassify" type="com.mfq.bean.HomeClassify"/>
        <typeAlias alias="AreaCity" type="com.mfq.bean.AreaCity"/>
        <typeAlias alias="Nurse" type="com.mfq.bean.Nurse"/>
        <typeAlias alias="PayRecord" type="com.mfq.bean.PayRecord"/>
        <typeAlias alias="Coupon" type="com.mfq.bean.coupon.Coupon"/>
        <typeAlias alias="CouponBatchInfo" type="com.mfq.bean.coupon.CouponBatchInfo"/>
        <typeAlias alias="Device" type="com.mfq.bean.Device"/>
        <typeAlias alias="OrderFreedom" type="com.mfq.bean.OrderFreedom"/>
        <typeAlias alias="OrderFreedomExample" type="com.mfq.bean.OrderFreedomExample"/>
    </typeAliases>

    <typeHandlers>
        <typeHandler javaType="com.mfq.bean.Status"
                     handler="com.mfq.handler.UserStatusEnumHandler"/>
        <typeHandler javaType="com.mfq.bean.Gender"
                     handler="com.mfq.handler.GenderEnumHandler"/>
        <typeHandler javaType="com.mfq.bean.Grade"
                     handler="com.mfq.handler.GradeEnumHandler"/>
        <typeHandler javaType="com.mfq.constants.ProductType"
                     handler="com.mfq.handler.ProductTypeEnumHandler"/>
        <typeHandler javaType="com.mfq.constants.BannerType"
                     handler="com.mfq.handler.BannerTypeEnumHandler"/>
    </typeHandlers>

    <mappers>
        <mapper resource="mybatis/mapping/AreaCityMapper.xml"/>
        <mapper resource="mybatis/mapping/FinanceBillMapper.xml"/>
        <mapper resource="mybatis/mapping/HomeBannerMapper.xml"/>
        <mapper resource="mybatis/mapping/HomeClassifyMapper.xml"/>
        <mapper resource="mybatis/mapping/HospitalMapper.xml"/>
        <mapper resource="mybatis/mapping/NotificationMapper.xml"/>
        <mapper resource="mybatis/mapping/NurseMapper.xml"/>
        <mapper resource="mybatis/mapping/OpLogsMapper.xml"/>
        <mapper resource="mybatis/mapping/SysUserMapper.xml"/>

        <mapper resource="mybatis/mapping/OrderInfoMapper.xml"/>
        <mapper resource="mybatis/mapping/PayRecordMapper.xml"/>
        <mapper resource="mybatis/mapping/PresentalMapper.xml"/>
        <mapper resource="mybatis/mapping/ProductClassifyMapper.xml"/>
        <mapper resource="mybatis/mapping/ProductDetailMapper.xml"/>
        <mapper resource="mybatis/mapping/ProductImgMapper.xml"/>
        <mapper resource="mybatis/mapping/ProductMapper.xml"/>
        <mapper resource="mybatis/mapping/SysAclMapper.xml"/>
        <mapper resource="mybatis/mapping/SysPassportMapper.xml"/>
        <mapper resource="mybatis/mapping/SysPermissionMapper.xml"/>
        <mapper resource="mybatis/mapping/SysRoleMapper.xml"/>
        <mapper resource="mybatis/mapping/UsersExtendMapper.xml"/>
        <mapper resource="mybatis/mapping/UsersMapper.xml"/>
        <mapper resource="mybatis/mapping/UsersQuotaMapper.xml"/>
        <mapper resource="mybatis/mapping/WechatMsgMapper.xml"/>
        <mapper resource="mybatis/mapping/coupon_batch_info.xml"/>
        <mapper resource="mybatis/mapping/coupon.xml"/>
        <mapper resource="mybatis/mapping/OrderFreedomMapper.xml"/>
        <mapper resource="mybatis/mapping/ActivityRecordMapper.xml"/>
        <mapper resource="mybatis/mapping/DeviceMapper.xml"/>
        <mapper resource="mybatis/mapping/InviteMoneyMapper.xml"/>
        <mapper resource="mybatis/mapping/InviteRecordMapper.xml"/>
        <mapper resource="mybatis/mapping/OperationRecordMapper.xml"/>
        <mapper resource="mybatis/mapping/UserFeedbackMapper.xml"/>

    </mappers>


</configuration>